                                    <li>
                                        <a class="dropdown-item" asp-area="" asp-controller="Redemption" asp-action="Attribute">Trả hàng nhập</a>
                                    </li>
                                    <li>
                                        <a class="dropdown-item" asp-area="" asp-controller="Redemption" asp-action="GroupRef">Xuất hàng</a>
                                    </li>
                                    <li>
                                        <a class="dropdown-item" asp-area="" asp-controller="PromotionGiftGroup" asp-action="Index">Trả hàng</a>
                                    </li>
                                    <li>
                                        <a class="dropdown-item" asp-area="" asp-controller="Redemption" asp-action="RedeemItemMall">Chuyển kho</a>
                                    </li>
                                    <li>
                                        <a class="dropdown-item" asp-area="" asp-controller="" asp-action="">Hủy hàng</a>
                                    </li>
                                    <li>
                                        <a class="dropdown-item" asp-area="" asp-controller="Redemption" asp-action="RedeemItemAddjust">Đếm hàng</a>
                                    </li>
                                </ul>                            
                        </li>                       
                        <li class="nav-item dropdown">
                            <a class="nav-link text-dark dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Báo cáo
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                               
                                        <li>
                                            <a class="dropdown-item" href="#">Thống kê chi tiết</a>
                                        </li>
                                        <li>
                                            <a class="dropdown-item" href="#">Sổ kho</a>
                                        </li>
                                        <li>
                                            <a class="dropdown-item" href="#">Sổ cái tháng </a>
                                        </li>
                                        <li>
                                            <a class="dropdown-item" href="#">Số cái năm</a>
                                        </li>
                                        
                                    </ul>
                                </li>
                               
                            
                        
                    </ul>
                    <!-- Left links -->
                </div>
            </div>
            <!-- Container wrapper -->
        </nav>
        <!-- Navbar -->


    </header>
    <div class="m-5 mb-10 mt-0 mh-100">

        <main role="main">
            <div class="justify-content-center">
                <h2 class="text-center">@ViewBag.PageTitle</h2>
            </div>
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted mt-2 mb-2" style="position:absolute!important">
        <div class="container">
            &copy; 2022 - CRMManager - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
