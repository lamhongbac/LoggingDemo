@page
@model WHMSolution.Pages.StockMaster.ImportModel
@{ 
}



<!DOCTYPE html>
<html>
<head>
    <title>Import excel to StockMaster</title>
</head>
<body>
    <h1>Import excel to Stock Master</h1>
    <form method="post" enctype="multipart/form-data">       
        <div class="row">
            <div class="col-8">
                <div class="mb-3">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="FileUpload.FormFile" class="control-label"></label>
                        <input asp-for="FileUpload.FormFile" class="form-control" type="file" name="file" id="FormFile">
                        <span asp-validation-for="FileUpload.FormFile" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="col-8">
                <div class="form-group">
                    <div class="mb-3">
                        <input class="btn btn-primary" type="submit" value="Upload" />
                    </div>
                </div>
            </div>
        </div>
    </form>
    <div >
        <a asp-page="/StockMaster/Index"> Back to Index</a>
    </div>
</body>
</html>
