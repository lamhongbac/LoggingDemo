<Window x:Class="WpfStudy.AutoMap.AutoMapDemo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfStudy.AutoMap"
        mc:Ignorable="d"
        Title="AutoMapDemo" Height="450" Width="800">
    <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
        <Button HorizontalAlignment="Center" Content="Simple Map"
                Command="{Binding AutoMapCommand}" 
                Height="30" Width="150" Padding="12,6" Margin="12,0"/>
        <Button HorizontalAlignment="Center" Content="Object to Prim"
                Command="{Binding AutoMapCommand2}" 
                Height="30" Width="150" Padding="12,6" Margin="12,0"/>
        <Button HorizontalAlignment="Center" Content="Reverse"
                Command="{Binding AutoMapCommand3}" 
                Height="30" Width="150" Padding="12,6" Margin="12,0"/>
    </StackPanel>
</Window>
